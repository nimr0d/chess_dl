not_newline = _{!("\n") ~ ANY }
attr_chars = _{ !("]" | "\n" | " \"") ~ ANY }
attr = { attr_chars* }
val_chars = _{ !("\"]") ~ ANY }
val = { val_chars* }
text = _{ not_newline+ ~ "\n"+}
header_line = {"[" ~ attr ~ " \"" ~ val ~ "\"]" ~ "\n"+}
game = { header_line* ~ text}
games = { SOI ~ game* ~ EOI}